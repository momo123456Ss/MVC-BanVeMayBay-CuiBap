<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Bán máy bay</title>
    <style>
        .nav {
            background-color: #e1f8f8;
        }
    </style>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!--//meta tags ends here-->
    <!--booststrap-->
    <link href="/Content/css/bootstrap.css" rel="stylesheet" type="text/css" media="all">
    <!--//booststrap end-->
    <!--stylesheets-->
    <link href="/Content/css/style.css" rel='stylesheet' type='text/css' media="all">
    <!--//stylesheets-->
    <!-- font-awesome icons -->
    <link href="/Content/css/font-awesome.min.css" rel="stylesheet">
    <!-- //font-awesome icons -->
    <link rel="stylesheet" href="/Content/css/flexslider.css" type="text/css" media="screen" />
    <!-- partner js-->
    <link href="//fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">
    <!-- form đặt-->
    <link href="/Content/css/wickedpicker.css" rel="stylesheet" type='text/css' media="all" />
    <link rel="stylesheet" href="/Content/css/jquery-ui.css" />
    <link href="/Content/css/style.css" rel='stylesheet' type='text/css' />
    <!-- //Stylesheet -->
    <!--fonts-->
    <link href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Raleway:300,400,500,600,700" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Megrim" rel="stylesheet">
    <!--//fonts-->

    <script type="text/javascript">

        var domain = 'http://vebay24h.somee.com';

    </script>

    <script src="https://www.google.com/recaptcha/api.js" async defer></script>

</head>
<body>
    <div class="header-w3layouts">
        <div class="container">
            <div class="header-bar">
                <nav class="navbar navbar-default">
                    <div class="navbar-header navbar-left">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                            <span class="sr-only">Mua vé máy bay</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <h1>
                            @*<a class="navbar-brand" style="font-size:30px" href="#">
                                <marquee direction="right">Vé bay nhóm 5</marquee>
                            </a>*@
                        </h1>
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse navbar-right" id="bs-example-navbar-collapse-1">
                        <nav>
                            <ul class="nav navbar-nav">
                                <li class="active"><a href="~/Auth/TrangChu"><span class="fa fa-home banner-nav" aria-hidden="true"></span>Trang chủ</a></li>
q                                <li><a href="#"><span class="fa fa-info-circle banner-nav" aria-hiudden="true"></span>Quốc Tế</a></li>
                                <li><a href="#"><span class="fa fa-cogs banner-nav" aria-hidden="true"></span>quoc noi</a></li>
                                <li><a href="#"><span class="fa fa-picture-o banner-nav" aria-hidden="true"></span>Giới Thiệu</a></li>
                                <li><a href="#"><span class="fa fa-envelope-o banner-nav" aria-hidden="true"></span>Liên hệ</a></li>
                                <li id="regis">@Html.ActionLink("Đăng ký", "CreateWithCustomer", "Register"@*, routeValues: null, htmlAttributes: new { id = "registerLink" }*@)</li>
                                <li id="login">@Html.ActionLink("Đăng nhập", "Login", "Auth"@*, routeValues: null, htmlAttributes: new { id = "loginLink" }*@)</li>
                            </ul>

                        </nav>
                    </div>
                </nav>

            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
    <br />
    <br />
    <br /><br />
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Vé máy bay Nhóm 5</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
